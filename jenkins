pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                 echo 'hello'
                 sh 'mvn clean package'
            }
        }
        stage('Test') { 
            steps {
                 echo 'everything is ok'
                 sh 'mvn test'
            }
        }
        stage('Deploy') { 
            steps {
                 echo 'deploying'
            }
        }
    }
}
